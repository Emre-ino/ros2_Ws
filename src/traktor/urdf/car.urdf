<?xml version="1.0"?>

<robot xmlns:xacro="http://www.ros.org/wiki/xacro" name="moveon">
  


<link name="base_link">
  <inertial>
    <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
    <mass value="10"/>
    <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0"/>
  </inertial>
  <visual name="">
    <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/> <!--  <mesh filename="package://your_package/meshes/your_mesh.stl"/> -->
    <geometry>
      <box size="2.5 1.5 1"/>
    </geometry>
    <material name="">
      <color rgba="1.0 0.0 0.0 1.0"/>
      <texture filename=""/>
    </material>
  </visual>
  <collision>
    <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/> <!--  <mesh filename="package://your_package/meshes/your_mesh.stl"/> -->
    <geometry>
      <box size="2.5 1.5 1"/>
    </geometry>
  </collision>
</link>

<link name="koltuk">
  <inertial>
    <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
    <mass value="1"/>
    <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0"/>
  </inertial>
  <visual name="">
    <origin xyz="0 0 0 " rpy="0.0 0.0 0.0"/> <!--  <mesh filename="package://your_package/meshes/your_mesh.stl"/> -->
    <geometry>
      <box size="1.5 1.5 0.5"/>
    </geometry>
    <material name="">
      <color rgba="1.0 0.0 0.0 1.0"/>
      <texture filename=""/>
    </material>
  </visual>
  <collision>
    <origin xyz="1 1.5 0.5" rpy="0.0 0.0 0.0"/> <!--  <mesh filename="package://your_package/meshes/your_mesh.stl"/> -->
    <geometry>
      <box size="1.5 1.5 0.5"/>
    </geometry>
  </collision>
</link>

<joint name="base_link_to_koltuk" type="fixed">
  <parent link="base_link"/>
  <child link="koltuk"/>
  <origin xyz="-0.5 0 0.75" rpy="0 0 0"/>
  <axis xyz="0 0 0"/>
</joint>

<link name="direksiyon">
  <inertial>
    <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
    <mass value="1"/>
    <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0"/>
  </inertial>
  <visual name="">
    <origin xyz="0 0 0 " rpy="0.0 0.0 0.0"/> <!--  <mesh filename="package://your_package/meshes/your_mesh.stl"/> -->
    <geometry>
      <cylinder radius="0.2" length="0.05"/>
    </geometry>
    <material name="">
      <color rgba="0.0 0.0 1.0 1.0"/>
      <texture filename=""/>
    </material>
  </visual>
  <collision>
    <origin xyz="1 1.5 0.5" rpy="0.0 0.0 0.0"/> <!--  <mesh filename="package://your_package/meshes/your_mesh.stl"/> -->
    <geometry>
      <cylinder radius="0.2" length="0.05"/>
    </geometry>
  </collision>
</link>

<joint name="direksiyon_joint" type="revolute"> <!-- SAĞ TEKER DİFERANSİYEL BASE LİNKİ-->
  <parent link="base_link"/>
  <child link="direksiyon"/>
  <origin xyz="0.25 0 1" rpy="0 -1 0"/>
  <axis xyz="0 0 1"/>
  <limit effort="100" velocity="1" lower="-1" upper="1"/>
  <dynamics damping="0.0" friction="0.0"/>
</joint>


<!-- SAĞ ÖN TEKER  -->

<link name="sag_on_dif"> <!-- SAĞ TEKER DİFERANSİYEL -->
  <visual>
    <geometry>
      <cylinder length="0.05" radius="0.02"/>
    </geometry>
    <material name="black"/>
  </visual>
</link>

<joint name="sag_on_dif_joint" type="revolute"> <!-- SAĞ TEKER DİFERANSİYEL BASE LİNKİ-->
  <parent link="base_link"/>
  <child link="sag_on_dif"/>
  <origin xyz="0.75 -0.85 -0.5" rpy="1.5707 0 0"/>
  <axis xyz="0 1 0"/>
  <limit effort="100" velocity="1" lower="-1" upper="1"/>
  <dynamics damping="0.0" friction="0.0"/>
</joint>


<link name="sag_on_teker"> <!-- SAĞ TEKER -->
  <inertial>
    <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
    <mass value="10"/>
    <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0"/>
  </inertial>
  <visual name="">
    <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/> <!--  <mesh filename="package://your_package/meshes/your_mesh.stl"/> -->
    <geometry>
      <cylinder length="0.20" radius="0.25"/>
    </geometry>
    <material name="">
      <color rgba="0.0 0.0 0.0 1.0"/>
      <texture filename=""/>
    </material>
  </visual>
  <collision>
    <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/> <!--  <mesh filename="package://your_package/meshes/your_mesh.stl"/> -->
    <geometry>
      <cylinder length="0.3" radius="0.25"/>
    </geometry>
  </collision>
</link>


<joint name="sag_on_dif_sag_on_teker" type="continuous"> <!-- SAĞ TEKER DİFERANSİYEL VE SAĞ TEKER JOİNTİ-->
  <parent link="sag_on_dif"/>
  <child link="sag_on_teker"/>
  <origin xyz="0.0 0.0 0.0" rpy="0 0 0"/>
  <axis xyz="0 0 -1"/>
  <limit effort="100" velocity="100"/>
  <dynamics damping="0.0" friction="0.0"/>
</joint>

<!-- SAĞ ARKA TEKERLEKLER -->




<link name="sag_arka"> <!-- SAĞ ARKA TEKER -->
  <inertial>
    <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
    <mass value="10"/>
    <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0"/>
  </inertial>
  <visual name="">
    <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/> <!--  <mesh filename="package://your_package/meshes/your_mesh.stl"/> -->
    <geometry>
      <cylinder length="0.20" radius="0.4"/>
    </geometry>
    <material name="">
      <color rgba="0.0 0.0 0.0 1.0"/>
      <texture filename=""/>
    </material>
  </visual>
  <collision>
    <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/> <!--  <mesh filename="package://your_package/meshes/your_mesh.stl"/> -->
    <geometry>
      <cylinder length="0.3" radius="0.4"/>
    </geometry>
  </collision>
</link>


<joint name="sag_arka_dif_joint" type="continuous"> <!-- SAĞ ARKA TEKER DİFERANSİYEL VE SAĞ ARKA TEKER JOİNTİ-->
  <parent link="base_link"/>
  <child link="sag_arka"/>
    <origin xyz="-1.25 -0.85 -0.35" rpy="1.5707 0 0"/>
  <axis xyz="0 0 1"/>
  <limit effort="100" velocity="100"/>
  <dynamics damping="0.0" friction="0.0"/>
</joint>


<!-- SOL ÖN TEKERLEKLER -->

<link name="sol_on_dif"> <!-- SOL ÖN TEKER DİFERANSİYEL -->
  <visual>
    <geometry>
      <cylinder length="0.05" radius="0.02"/>
    </geometry>
    <material name="black"/>
  </visual>
</link>

<joint name="sol_on_dif_joint" type="revolute"> <!-- SOL ÖN TEKER DİFERANSİYEL BASE LİNKİ-->
  <parent link="base_link"/>
  <child link="sol_on_dif"/>
  <origin xyz="0.75 0.85 -0.5" rpy="1.5707 0 0"/>
  <axis xyz="0 1 0"/>
  <limit effort="100" velocity="1" lower="-1" upper="1"/>
  <dynamics damping="0.0" friction="0.0"/>
</joint>


<link name="sol_on"> <!-- SOL ÖN TEKER -->
  <inertial>
    <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
    <mass value="10"/>
    <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0"/>
  </inertial>
  <visual name="">
    <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/> <!--  <mesh filename="package://your_package/meshes/your_mesh.stl"/> -->
    <geometry>
      <cylinder length="0.20" radius="0.25"/>
    </geometry>
    <material name="">
      <color rgba="0.0 0.0 0.0 1.0"/>
      <texture filename=""/>
    </material>
  </visual>
  <collision>
    <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/> <!--  <mesh filename="package://your_package/meshes/your_mesh.stl"/> -->
    <geometry>
      <cylinder length="0.3" radius="0.25"/>
    </geometry>
  </collision>
</link>


<joint name="sol_on_dif_sol_on_teker" type="continuous"> <!-- SOL ÖN TEKER DİFERANSİYEL VE SOL ÖN TEKER JOİNTİ-->
  <parent link="sol_on_dif"/>
  <child link="sol_on"/>
  <origin xyz="0.0 0.0 0.0" rpy="0 0 0"/>
  <axis xyz="0 0 -1"/>
  <limit effort="100" velocity="100"/>
  <dynamics damping="0.0" friction="0.0"/>
</joint>


<!-- SOL ARKA TEKERLEKLER -->

<link name="sol_arka"> <!-- SOL ARKA TEKER -->
  <inertial>
    <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
    <mass value="10"/>
    <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0"/>
  </inertial>
  <visual name="">
    <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/> <!--  <mesh filename="package://your_package/meshes/your_mesh.stl"/> -->
    <geometry>
      <cylinder length="0.20" radius="0.4"/>
    </geometry>
    <material name="">
      <color rgba="0.0 0.0 0.0 1.0"/>
      <texture filename=""/>
    </material>
  </visual>
  <collision>
    <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/> <!--  <mesh filename="package://your_package/meshes/your_mesh.stl"/> -->
    <geometry>
      <cylinder length="0.3" radius="0.4"/>
    </geometry>
  </collision>
</link>


<joint name="sol_arka_dif_joint" type="continuous"> <!-- SOL ARKA TEKER DİFERANSİYEL VE SOL ARKA TEKER JOİNTİ-->
  <parent link="base_link"/>
  <child link="sol_arka"/>
    <origin xyz="-1.25 0.85 -0.35" rpy="1.5707 0 0"/>
  <axis xyz="0 0 -1"/>
  <limit effort="100" velocity="100"/>
  <dynamics damping="0.0" friction="0.0"/>
</joint>



<gazebo>
    <plugin name="gazebo_ros_ackermann_drive" filename="libgazebo_ros_ackermann_drive.so">

  <ros>
    <namespace>ackermann</namespace>
    <remapping>cmd_vel:=cmd_vel</remapping>
    <remapping>odom:=odom</remapping>
    <remapping>distance:=distance</remapping>
  </ros>

  <update_rate>100.0</update_rate>

  <!-- wheels -->
  <front_left_joint>sol_on_dif_sol_on_teker</front_left_joint>
  <front_right_joint>sag_on_dif_sag_on_teker</front_right_joint>
  <rear_left_joint>sol_arka_dif_joint</rear_left_joint>
  <rear_right_joint>sag_arka_dif_joint</rear_right_joint>
  <left_steering_joint>sol_on_dif_joint</left_steering_joint>
  <right_steering_joint>sag_on_dif_joint</right_steering_joint>
  <steering_wheel_joint>direksiyon_joint</steering_wheel_joint>
  

  <!-- Max absolute steer angle for tyre in radians-->
  <!-- Any cmd_vel angular z greater than this would be capped -->
  <max_steer>0.6458</max_steer>

  <!-- Max absolute steering angle of steering wheel -->
  <max_steering_angle>7.85</max_steering_angle>

  <!-- Max absolute linear speed in m/s -->
  <max_speed>20</max_speed>

  <!-- PID tuning -->
  <left_steering_pid_gain>1500 0 1</left_steering_pid_gain>
  <left_steering_i_range>0 0</left_steering_i_range>
  <right_steering_pid_gain>1500 0 1</right_steering_pid_gain>
  <right_steering_i_range>0 0</right_steering_i_range>
  <linear_velocity_pid_gain>1000 0 1</linear_velocity_pid_gain>
  <linear_velocity_i_range>0 0</linear_velocity_i_range>

  <!-- output -->
  <publish_odom>true</publish_odom>
  <publish_odom_tf>true</publish_odom_tf>
  <publish_wheel_tf>true</publish_wheel_tf>
  <publish_distance>true</publish_distance>

  <odometry_frame>odom</odometry_frame>
  <robot_base_frame>base_link</robot_base_frame>

  </plugin>
</gazebo>

  <gazebo>
    <plugin name="gazebo_ros_ackermann_drive" filename="libgazebo_ros_ackermann_drive.so">
      <ros>
        <namespace>demo</namespace>
        <remapping>cmd_vel:=cmd_demo</remapping>
        <remapping>odom:=odom_demo</remapping>
        <remapping>distance:=distance_demo</remapping>
      </ros>
      <update_rate>100.0</update_rate>
      <!-- wheels -->
      <front_left_joint>sol_on_dif_sol_on_teker</front_left_joint>
      <front_right_joint>sag_on_dif_sag_on_teker</front_right_joint>
      <rear_left_joint>sol_arka_dif_joint</rear_left_joint>
      <rear_right_joint>sag_arka_dif_joint</rear_right_joint>
      <left_steering_joint>sol_on_dif_joint</left_steering_joint>
      <right_steering_joint>sag_on_dif_joint</right_steering_joint>
      <steering_wheel_joint>direksiyon_joint</steering_wheel_joint>
      <!-- Max absolute steer angle for tyre in radians-->
      <!-- Any cmd_vel angular z greater than this would be capped -->
      <max_steer>0.6458</max_steer>
      <!-- Max absolute steering angle of steering wheel -->
      <max_steering_angle>7.85</max_steering_angle>
      <!-- Max absolute linear speed in m/s -->
      <max_speed>20</max_speed>
      <!-- PID tuning -->
      <left_steering_pid_gain>1500 0 1</left_steering_pid_gain>
      <left_steering_i_range>0 0</left_steering_i_range>
      <right_steering_pid_gain>1500 0 1</right_steering_pid_gain>
      <right_steering_i_range>0 0</right_steering_i_range>
      <linear_velocity_pid_gain>1000 0 1</linear_velocity_pid_gain>
      <linear_velocity_i_range>0 0</linear_velocity_i_range>
      <!-- output -->
      <publish_odom>true</publish_odom>
      <publish_odom_tf>true</publish_odom_tf>
      <publish_wheel_tf>true</publish_wheel_tf>
      <publish_distance>true</publish_distance>
      <odometry_frame>odom_demo</odometry_frame>
      <robot_base_frame>base_link</robot_base_frame>
    </plugin>
  </gazebo>




</robot>

